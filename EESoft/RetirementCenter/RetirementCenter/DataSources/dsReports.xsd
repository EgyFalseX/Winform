                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT        PersonId, DofatSarfId, SarfTypeedadId, sarfdatefrom, sarfdateto, Edafat,
                             (SELECT        WarasaType
                                FROM            CDWarasaType
                                WHERE        (WarasaTypeId =
                                                             (SELECT        WarasaTypeId
                                                                FROM            TBLWarasa
                                                                WHERE        (PersonId = TBLWarasaSarf.PersonId)))) AS WarasaType,
                             (SELECT        MMashatId
                                FROM            TBLWarasa AS TBLWarasa_11
                                WHERE        (PersonId = TBLWarasaSarf.PersonId)) AS MMashatId,
                             (SELECT        personName
                                FROM            TBLWarasa AS TBLWarasa_10
                                WHERE        (PersonId = TBLWarasaSarf.PersonId)) AS personName,
                             (SELECT        personNID
                                FROM            TBLWarasa AS TBLWarasa_9
                                WHERE        (PersonId = TBLWarasaSarf.PersonId)) AS personNID,
                             (SELECT        SyndicateId
                                FROM            TBLWarasa AS TBLWarasa_8
                                WHERE        (PersonId = TBLWarasaSarf.PersonId)) AS SyndicateId,
								(SELECT SubCommitte FROM CDSubCommitte WHERE SubCommitteId = (SELECT SubCommitteId FROM TBLWarasa AS TBLWarasa_7 WHERE (PersonId = TBLWarasaSarf.PersonId))) AS SubCommitte,
(SELECT SubCommitteId FROM TBLWarasa AS TBLWarasa_7 WHERE (PersonId = TBLWarasaSarf.PersonId)) AS SubCommitteId,
                             (SELECT        MMashatName
                                FROM            TBLMashat
                                WHERE        (MMashatId =
