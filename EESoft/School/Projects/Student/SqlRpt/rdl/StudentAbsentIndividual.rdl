    <DataSet Name="DataSetstu_code">
      <Fields>
        <Field Name="stu_code">
          <DataField>stu_code</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Stu_Name">
          <DataField>Stu_Name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>DataSourceStudent</DataSourceName>
        <CommandText>SELECT     stu.student_t.stu_code, 
stu.TBLstudent.FIRST_NAME + N' ' + stu.TBLstudent.FATHER_NAME + N' ' + stu.TBLstudent.GF_NAME + N' ' + stu.TBLstudent.FAMILY_NAME AS Stu_Name
FROM         stu.student_t INNER JOIN
                      stu.TBLstudent ON stu.student_t.stu_code = stu.TBLstudent.stu_code
WHERE     (stu.student_t.asase_code = @asase_code) AND (stu.student_t.alsofof_code = @alsofof_code) AND (stu.student_t.fasl_code = @fasl_code)</CommandText>
        <QueryParameters>
          <QueryParameter Name="@asase_code">
