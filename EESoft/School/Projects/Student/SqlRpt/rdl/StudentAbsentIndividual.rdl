        </Field>
        <Field Name="eyab_Name">
          <DataField>eyab_Name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="waleaalkamr_phone">
          <DataField>waleaalkamr_phone</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="addres">
          <DataField>addres</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>DataSourceStudent</DataSourceName>
        <CommandText>SELECT stu_code,asase_code, eyab_date, remarks,
        (Select alsofof_name From CDAlsofof Where alsofof_code = (Select alsofof_code From stu.student_t Where stu_code = stu.stu_eyab.stu_code And asase_code = stu.stu_eyab.asase_code)) AS alsofof_name,
        (Select fasl_name From CDEFasl Where fasl_code = (Select fasl_code From stu.student_t Where stu_code = stu.stu_eyab.stu_code And asase_code = stu.stu_eyab.asase_code)) AS fasl_name,
        (Select eyab_Name From cdEyab_code Where eyab_code = stu.stu_eyab.eyab_code) AS eyab_Name,
        (Select FIRST_NAME + ' ' + FATHER_NAME + ' ' + GF_NAME + ' ' + FAMILY_NAME From stu.TBLstudent  Where  stu_code = stu.stu_eyab.stu_code) AS Stu_Name,
        (Select addres From stu.TBLstudent  Where  stu_code = stu.stu_eyab.stu_code) AS addres,
        (Select waleaalkamr_phone From stu.TBLstudent  Where  stu_code = stu.stu_eyab.stu_code) AS waleaalkamr_phone
    FROM stu.stu_eyab
    WHERE asase_code = @asase_code And stu_code = @stu_code  And eyab_date BETWEEN @FromDate AND @ToDate</CommandText>
        <QueryParameters>
          <QueryParameter Name="@asase_code">
            <Value>=Parameters!asase_code.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@stu_code">
            <Value>=Parameters!stu_code.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@FromDate">
            <Value>=Parameters!FromDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@ToDate">
            <Value>=Parameters!ToDate.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
