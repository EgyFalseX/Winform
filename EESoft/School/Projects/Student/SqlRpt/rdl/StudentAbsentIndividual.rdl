          </QueryParameter>
          <QueryParameter Name="@alsofof_code">
            <Value>=Parameters!alsofof_code.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@fasl_code">
            <Value>=Parameters!fasl_code.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
