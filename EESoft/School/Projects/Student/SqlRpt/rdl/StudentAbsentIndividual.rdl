  <Code>Public Function GetWeekArName(ByVal name as String) as String

IF name = "Sat" Then

Return "السبت"

End IF

IF name = "Sun" Then

Return "الاحد"

End IF

IF name = "Mon" Then

Return "الاثنين"

End IF

IF name = "Tue" Then

Return "الثلاثاء"

End IF

IF name = "Wed" Then

Return "الاربعاء"

End IF

IF name = "Thu" Then

Return "الخميس"

End IF

IF name = "Fri" Then

Return "الجمعه"

End IF

Return name

End Function
Public Function GetMonthArName(ByVal name as String) as String

IF name = "January" Then

Return "يناير"

End IF

IF name = "February" Then

Return "فبراير"

End IF

IF name = "March" Then

Return "مارس"

End IF

IF name = "April" Then

Return "أبريل"

End IF

IF name = "May" Then

Return "مايو"

End IF

IF name = "June" Then

Return "يونيو"

End IF

IF name = "July" Then

Return "يوليو"

End IF

IF name = "August" Then

Return "أغسطس"

End IF

IF name = "September" Then

Return "سبتمبر"

End IF

IF name = "October" Then

Return "أكتوبر"

End IF

IF name = "November" Then

Return "نوفمبر"

End IF

IF name = "December" Then

Return "ديسمبر"

End IF

Return name

End Function</Code>
